{
  "name": "crcms/crcms",
  "description": "The CrCms Framework.",
  "keywords": [
    "crcms",
    "passport",
    "mall",
    "cms",
    "laravel"
  ],
  "type": "project",
  "require": {
    "php": "^7.2.4",
    "crcms/framework": "dev-4.x-dev"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "laravel/tinker": "^1.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "CrCms\\App\\": "app/",
      "CrCms\\Extensions\\": "extensions/",
      "CrCms\\Modules\\": "modules/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CrCms\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php bin/crcms key:generate --ansi"
    ],
    "post-autoload-dump": [
      "@php bin/artisan make:directory storage",
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php bin/crcms package:discover --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.4"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  }
}
